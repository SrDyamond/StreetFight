<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        layout="@layout/map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/button_change_clan4"
        android:layout_width="370dp"
        android:layout_height="75dp"
        android:layout_marginTop="25dp"
        android:text="button_change_clan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_change_clan3" />

    <Button
        android:id="@+id/button_change_clan3"
        android:layout_width="370dp"
        android:layout_height="75dp"
        android:layout_marginTop="45dp"
        android:text="button_change_clan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_create_clan" />

    <Button
        android:id="@+id/button_create_clan"
        android:layout_width="160dp"
        android:layout_height="65dp"
        android:layout_marginTop="136dp"
        android:text="button_create_clan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_change_clan"
        app:layout_constraintTop_toBottomOf="@+id/textView_clan_score" />

    <ImageView
        android:id="@+id/logo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="440dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_clan"
        app:layout_constraintVertical_bias="1.0"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView_clan"
        android:layout_width="296dp"
        android:layout_height="101dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_user" />

    <TextView
        android:id="@+id/textView_clan_score"
        android:layout_width="115dp"
        android:layout_height="101dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_user_score" />

    <TextView
        android:id="@+id/textView_user"
        android:layout_width="296dp"
        android:layout_height="97dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_user_score"
        android:layout_width="115dp"
        android:layout_height="97dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_change_clan"
        android:layout_width="160dp"
        android:layout_height="65dp"
        android:layout_marginTop="136dp"
        android:text="button_change_clan"
        app:layout_constraintEnd_toStartOf="@+id/button_create_clan"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_clan" />

</androidx.constraintlayout.widget.ConstraintLayout>